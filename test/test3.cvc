
export int gcd(int a, int b) {
    
    int i = 1;
    int gcd = 1;

    while((a > i || b > i)) {
        if(a%i==0 && b%i==0) {
            gcd = i;
        }
        i += 1;
    }
    return gcd;
}

export int fac(int n) {
    int i = n;
    int total = n;

    while(i > n) {
        total += n * i;

        i -= 1;
    }
    return total;
}

export int fib(int n) {
    int fib1 = 0;
    int fib2 = 1;

    int next = fib1 + fib2;

    int i = 3;

    while(i < n) {

        fib1 = fib2;
        fib2 = next;

        next = fib1 + fib2;
        
        i += 1;
    }

    return next;
}

export bool isprime(int b) {

    int i = 2;
    bool isprime = true;

    
    if(b < 2) {
        return false;
    }
    
    while(i < b) {
        if(i % b == 0) {
            isprime = false;
            
        }
        i += 1;
    }

    return isprime;
}

